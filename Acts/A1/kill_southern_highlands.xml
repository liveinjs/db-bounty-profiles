<Profile>
<Name>General Southern Highlands Bounty</Name>
<KillMonsters>True</KillMonsters>
<PickupLoot>true</PickupLoot>
<GameParams act="OpenWorld" resumeFromSave="False" isPrivate="True" numGames="-1" />
<Order>

	<If condition="Me.IsInTown==False"><TrinityTownPortal questId="1" /></If>		
	<UseWaypoint waypointNumber="13" questId="1"/>	
	<WaitTimer questId="1" stepId="2" waitTime="2000" />		

    <!-- Look for him -->
	<LogMessage quest="1" step="2" output="Swiggity swooty, we're roaming for that booty..." />
	<TrinityExploreDungeon stayAfterBounty="False" questId="1" stepId="2" until="BountyComplete" boxTolerance="0.01" boxSize="25" >
		<IgnoreScenes>
			 <IgnoreScene sceneName="_Chokepoint_" />
		</IgnoreScenes>
	</TrinityExploreDungeon>
	
	<LoadProfile file="act1.xml" />
	
</Order>
</Profile>