            <PriorityScene sceneName="Exit" />
          </PrioritizeScenes>
        </TrinityExploreDungeon>
        <MoveToMapMarker questId="349210" stepId="2" markerNameHash="-1693984105" interactRange="10" actorId="175482" />
      </If>
      <!-- Explore The Barracks Level 2 -->
      <If condition="CurrentWorldId == 221749">
		<TrinitySetQuesting />
        <TrinityExploreDungeon questId="349210" stepId="2" until="FullyExplored" boxTolerance="0.01" boxSize="15" ignoreMarkers="False" />
      </If>
    </HaveBounty>

    <LoadProfile file="act3.xml" />
  </Order>
</Profile>