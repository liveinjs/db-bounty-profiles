<Profile>
  <Name>Kill_Obis</Name>
  <KillMonsters>True</KillMonsters>
  <PickupLoot>true</PickupLoot>
  <GameParams act="OpenWorld" resumeFromSave="False" isPrivate="True" numGames="-1" />

  <Order>
    <If condition="HasQuest(369316)">

      <While condition="CurrentWorldId != 93099 and CurrentWorldId != 81019">
        <UseWaypoint questId="1" waypointNumber="27" />
        <WaitTimer questId="1" stepId="1" waitTime="2500"/>
      </While>

      <If condition="CurrentWorldId == 93099">
        <SafeMoveTo questId="369316" stepId="2" x="4265" y="4185" z="-25" pathPrecision="5" pathPointLimit="250" />
        <!--<MoveToMapMarker questId="369316" stepId="2" markerNameHash="-1078336204" actorId="176002" interactRange="20"
pathPrecision="5" pathPointLimit="250" isPortal="True" destinationWorldId="81019" />-->
        <UsePortal questId="369316" stepId="1" actorId="176002" destinationWorldId="81019" interactRange="20" portalMarkerHash="-1078336204"/>
      </If>

      <If condition="CurrentWorldId == 81019">
        <TrinityExploreDungeon questId="369316" stepId="1" until="ObjectFound" actorId="367335" boxTolerance="0.20" boxSize="20" />
        <WaitTimer questId="1" stepId="2" waitTime="1000" />
        <!-- If not completed, keep killing things until it is complete-->
        <If condition="HasQuest(369316)">
          <LogMessage quest="1" step="1" output="Found named but still not done, clearing some more." />
          <TrinityExploreDungeon questId="369316" stepId="1" until="BountyComplete" boxTolerance="0.10" boxSize="15" />
        </If>
      </If>
    </If>

    <LoadProfile file="act3.xml" />
  </Order>
</Profile>