<Profile>
  <Name>Kill_Riplash</Name>
  <KillMonsters>True</KillMonsters>
  <PickupLoot>true</PickupLoot>
  <GameParams act="OpenWorld" resumeFromSave="False" isPrivate="True" numGames="-1" />

  <Order>
    <HaveBounty questId="349236">

      <While condition="CurrentWorldId != 79401 and CurrentWorldId != 80763">
        <UseWaypoint questId="1" waypointNumber="35" />
        <WaitTimer questId="1" stepId="1" waitTime="2500"/>
      </While>
      <If condition="CurrentWorldId == 79401">
        <SafeMoveTo questId="349236" stepId="2" x="1013" y="975" z="-10" pathPrecision="5" pathPointLimit="250" statusText="" />
        <SafeMoveTo questId="349236" stepId="2" x="964" y="1096" z="-30" pathPrecision="5" pathPointLimit="250" statusText="" />
        <SafeMoveTo questId="349236" stepId="2" x="1059" y="1140" z="-40" pathPrecision="5" pathPointLimit="250" statusText="" />
        <SafeMoveTo questId="349236" stepId="2" x="1090" y="1038" z="-84" pathPrecision="5" pathPointLimit="250" statusText="" />
        <MoveToMapMarker questId="349236" stepId="2" x="1090" y="1038" z="-84" markerNameHash="2083727833" actorId="176001" interactRange="20"
        pathPrecision="5" pathPointLimit="250" isPortal="True" destinationWorldId="80763" />
      </If>
      <If condition="CurrentWorldId == 80763">
        <TrinityExploreDungeon questId="349236" stepId="1" until="ObjectFound" actorId="220814" boxTolerance="0.10" boxSize="15" />
        <WaitTimer questId="1" stepId="2" waitTime="1000" />
        <!-- If not completed, keep killing things until it is complete-->
        <HaveBounty questId="349236">
          <LogMessage quest="1" step="1" output="Found named but still not done, clearing some more." />
          <TrinityExploreDungeon questId="349236" stepId="1" until="FullyExplored" boxTolerance="0.10" boxSize="15" />
        </HaveBounty>
      </If>
    </HaveBounty>

    <LoadProfile file="act3.xml" />
  </Order>
</Profile>