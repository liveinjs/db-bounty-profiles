<Profile>
  <Name>Kill_Axgore</Name>
  <KillMonsters>True</KillMonsters>
  <PickupLoot>true</PickupLoot>
  <GameParams act="OpenWorld" resumeFromSave="False" isPrivate="True" numGames="-1" />

  <Order>
    <If condition="HasQuest(346232)">

      <While condition="CurrentWorldId != 119290">
        <UseWaypoint questId="1" waypointNumber="38" />
        <WaitTimer questId="1" stepId="1" waitTime="2500"/>
      </While>

      <If condition="CurrentWorldId == 119290">
        <TrinityExploreDungeon questId="346232" stepId="1" until="ObjectFound" actorId="220884" boxTolerance="0.40" boxSize="20" />
        <WaitTimer questId="1" stepId="2" waitTime="1000" />
        <!-- If not completed, keep killing things until it is complete-->
        <If condition="HasQuest(346232)">
          <LogMessage quest="1" step="1" output="Found named but still not done, clearing some more." />
          <TrinityExploreDungeon questId="346232" stepId="1" until="BountyComplete" boxTolerance="0.10" boxSize="15"  />
        </If>
      </If>
    </If>

    <LoadProfile file="act3.xml" />
  </Order>
</Profile>