    <While condition="HasQuest(368607) and CurrentWorldId==351793">
      <ExploreDungeon questId="368607" until="ObjectiveFound" boxSize="10" boxTolerance="0.01" setNodesExploredAutomatically="False" />
      <MoveToObjective questId="368607" />
      <ExploreDungeon questId="368607" until="FullyExplored" boxSize="10" boxTolerance="0.01" setNodesExploredAutomatically="False" />
