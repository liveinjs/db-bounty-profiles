    <If condition="CurrentWorldId==263494 and not (HasQuest(368607) or HasQuest(375348) or HasQuest(368613) or HasQuest(359919))">
      <TrinityExploreDungeon questId="1" until="ObjectiveFoundOrBountyComplete" boxSize="60" boxTolerance="0.01" />
      <MoveToObjective questId="1" />
    </If>
    <While condition="not (ZetaDia.IsInTown) and ZetaDia.ActInfo.ActiveBounty and not (HasQuest(368607) or HasQuest(375348) or HasQuest(368613) or HasQuest(359919))">
      <TrinityExploreDungeon questId="1" until="ObjectiveFoundOrBountyComplete" boxSize="35" boxTolerance="0.01" />
      <MoveToObjective questId="1" />
    </While>

    <If condition="CurrentWorldId==263494 and not (HasQuest(368607) or HasQuest(375348) or HasQuest(368613) or HasQuest(359919))">
      <WaitTimer questId="1" waitTime="1000" />
      <!-- Start Explore -->
      <TrinityExploreDungeon questId="1" until="ObjectiveFoundOrBountyComplete" exitNameHash="0" actorId="0" pathPrecision="60" boxSize="45" boxTolerance="0.01" objectDistance="45" />
    </If>

    <If condition="not (ZetaDia.ActInfo.ActiveBounty)">
      <LogMessage output="Bounty is Finished" />
      <TrinityTownPortal questId="1" waitTime="5000" />
      <LoadProfile profile="Act5.xml" />
    </If>
    <If condition="ZetaDia.ActInfo.ActiveBounty">
      <ReloadProfile />
    </If>
  </Order>
</Profile>